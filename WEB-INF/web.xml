<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <servlet>
    <servlet-name>Hello</servlet-name>
    <servlet-class>cn.com.servlets.HelloServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Show</servlet-name>
    <servlet-class>cn.com.servlets.EmpBeanInfo</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Hello</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Show</servlet-name>
    <url-pattern>/emp</url-pattern>
  </servlet-mapping>

</web-app>